            "title": "Total Repositories in Database",
            "type": "stat",
            "gridPos": {
                "x": 0,
                "y": 0,
                "w": 6,
                "h": 4
            },
            "datasource": null,
            "options": {
                "colorMode": "value",
                "graphMode": "none",
                "justifyMode": "auto",
                "orientation": "auto",
                "reduceOptions": {
                    "calcs": [
                        "last"
                    ],
                    "fields": "/.*/",
                    "values": false
                },
                "textMode": "value"
            },
            "pluginVersion": "10.0.0",
            "targets": [
                {
                    "expr": "augur_collection_status_full_total{database=\"augur\"}",
                    "refId": "A"
                }
            ]
        },
        {
            "title": "Collection Status Counts",
            "type": "stat",
            "gridPos": {
                "x": 6,
                "y": 0,
                "w": 18,
                "h": 4
            },
            "datasource": null,
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "mappings": [],
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "green",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 1
                            },
                            {
                                "color": "red",
                                "value": 5
                            }
                        ]
                    }
                },
                "overrides": []
            },
            "options": {
                "colorMode": "value",
                "graphMode": "none",
                "justifyMode": "auto",
                "orientation": "horizontal",
                "reduceOptions": {
                    "calcs": [
                        "last"
                    ],
                    "fields": "/(Pending|Error|Ignore)/",
                    "values": false
                },
                "textMode": "name_value"
            },
            "pluginVersion": "10.0.0",
            "targets": [
                {
                    "expr": "augur_collection_status_counts{collection_state=\"Pending\", database=\"augur\"}",
                    "legendFormat": "Pending",
                    "refId": "A"
                },
                {
                    "expr": "augur_collection_status_counts{collection_state=\"Error\", database=\"augur\"}",
                    "legendFormat": "Error",
                    "refId": "B"
                },
                {
                    "expr": "augur_collection_status_counts{collection_state=\"Ignore\", database=\"augur\"}",
                    "legendFormat": "Ignored",
                    "refId": "C"
                }
            ]
        },
        {
            "title": "Collected Repositories by Days Ago",
            "type": "timeseries",
            "gridPos": {
                "x": 0,
                "y": 4,
                "w": 12,
                "h": 8
            },
            "datasource": null,
            "options": {
                "tooltip": {
                    "mode": "single",
                    "sort": "none"
                }
            },
            "fieldConfig": {
                "defaults": {
                    "custom": {
                        "axisPlacement": "auto",
                        "drawStyle": "line",
                        "fillOpacity": 20,
                        "lineStyle": {
                            "fill": "solid"
                        },
                        "pointSize": 5,
                        "showPoints": "always"
                    },
                    "unit": "short"
                },
                "overrides": []
            },
            "pluginVersion": "10.0.0",
            "targets": [
                {
                    "expr": "augur_collected_recently_counts{collection_phase!=\"None\"}",
                    "legendFormat": "{{collection_phase}}",
                    "refId": "A"
                }
            ],
            "xaxis": {
                "mode": "series",
                "name": "Days Ago (From Last Scrape)",
                "show": true
            }
        },
        {
            "title": "Recent Collection Success Rate (Last ${recent_days} Days)",
            "type": "gauge",
            "gridPos": {
                "x": 12,
                "y": 4,
                "w": 12,
                "h": 8
            },
            "datasource": null,
            "fieldConfig": {
                "defaults": {
                    "color": {
                        "mode": "thresholds"
                    },
                    "max": 1,
                    "min": 0,
                    "unit": "percentunit",
                    "thresholds": {
                        "mode": "absolute",
                        "steps": [
                            {
                                "color": "red",
                                "value": null
                            },
                            {
                                "color": "yellow",
                                "value": 0.8
                            },
                            {
                                "color": "green",
                                "value": 0.95
                            }
                        ]
                    }
                },
                "overrides": []
            },
            "options": {
                "displayMode": "lcd",
                "showThresholdLabels": false,
                "showThresholdMarkers": true
            },
            "pluginVersion": "10.0.0",
            "targets": [
                {
                    "expr": "sum(augur_collected_recently_counts{days_ago!=\"None\"}) / scalar(augur_collection_status_total)",
                    "legendFormat": "Success Rate",
                    "refId": "A"
                }
            ]
        },
        {
            "title": "Query Latency (P50/P90/P99) Histogram",
            "type": "heatmap",
            "gridPos": {
                "x": 0,
                "y": 12,
                "w": 24,
                "h": 8
            },
            "datasource": null,
            "options": {
                "color": {
                    "mode": "spectrum",
                    "scheme": "Oranges"
                },
                "combine": false,
                "reverseY": false,
                "dataLinkMode": "none",
                "yAxis": {
                    "label": "Latency (seconds)",
                    "unit": "s",
                    "decimals": 3
                }
            },
            "pluginVersion": "10.0.0",
            "targets": [
                {
                    "expr": "rate(query_latency_bucket[5m])",
                    "format": "heatmap",
                    "legendFormat": "{{query}} (Le: {{le}})",
                    "refId": "A"
                }
            ]
        }
    ],
